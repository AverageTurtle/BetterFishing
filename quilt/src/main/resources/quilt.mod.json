{
  "schema_version": 1,
  "mixin": [
    "betterfishing.mixins.json",
    "betterfishing-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "betterfishing",
    "version": "${version}",
    "metadata": {
      "name": "Better Fishing",
      "description": "fish.",
      "contributors": {
        "AverageTurtle": "Owner"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/betterfishing/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "net.betterfishing.quilt.BetterFishingModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      },
      {
        "id": "architectury",
        "version": ">=9.0.0"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}